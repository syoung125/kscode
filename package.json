    "styled-reset": "^4.3.4",
    "webpack": "^5.38.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/marked": "^2.0.3",
    "@types/react": "17.0.8",
    "@types/styled-components": "^5.1.9",
    "@types/webpack-env": "^1.16.0",
    "babel-plugin-transform-define": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "gh-pages": "^3.2.0",
    "jest": "^27.0.6",

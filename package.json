{
  "name": "kscode",
  "homepage": "http://syoung125.github.io/Ko-Seoyoung-Code",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write **/*.{ts,tsx}",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "tsc": "tsc",
    "test": "jest",
    "dev": "next dev",
    "build": "next build",
